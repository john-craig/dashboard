{
    "name": "Dashboard",
    "description": "A personalized landing page",
    "version": "0.1",
    "manifest_version": 2,

    "chrome_url_overrides": {
        "newtab": "index.html"
    },

    "permissions": [
        "identity",
        "tabs",
        "https://apis.google.com/"
      ],

    "background": {
        "scripts": ["backend.js"],
        "persistent": false
      },

    "content_security_policy": "script-src 'self' https://apis.google.com/; object-src 'self'",
    
    "oauth": {
        "clientid": "580956459961-uo675crpeuq37jltt49pqmqpdhii3egh.apps.googleusercontent.com",
        "scopes": [
            "profile"
          ]
    },

    "key": ""
}